{% extends 'base.html.twig' %}
{% block stylesheets %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
            <link rel="stylesheet" href="../LoginCSS.css">
{% endblock %}
{% block content %}
    <div class="infoEntrCardBody">
        <div class="divGaucheInfos">
            <h2>{{ uneEntreprise.ENT_RaisonSociale}}</h2>
            <h3>{{ uneEntreprise.ENT_RUE}}</h3>
            <h3>{{ uneEntreprise.ENT_ComplementAdresse}}</h3>
            <h3>{{ uneEntreprise.ENT_CP}} {{ uneEntreprise.ENT_Ville}}</h3>
            <h1>Contact</h1>
            <div class="contactEntr">
            {% for PERSONNE in unePersonne  %}
                <div class="contactUnite">
                    <h4>{{ PERSONNE.PER_NOM}} {{ PERSONNE.PER_PRENOM}}</h4>
                    <h4>{{ PERSONNE.PER_MAIL}}</h4>
                    <h4>{{ PERSONNE.PER_TEL}}</h4>
                </div>
            {% endfor %}
            </div>
           
        </div>
        <div class="divDroiteMaps">
            <iframe src="https://maps.google.com/maps?width=100%25&amp;height=auto&amp;hl=fr&amp;q={{uneEntreprise.ENT_RUE}}+{{uneEntreprise.ENT_Ville}}&amp;t=p&amp;z=13&amp;ie=UTF8&amp;iwloc=B&amp;output=embed" width="100%" height="100%"></iframe>
        </div>
    </div>
{% endblock %}
